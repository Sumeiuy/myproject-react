{
  "presets": ["es2015", "stage-2", "react"],
  "plugins": [
    [
      "import", {
        "libraryName": "antd",
        "style": true
      }
    ],
    "add-module-exports",
    "transform-runtime",
    "transform-decorators-legacy"
  ],
  "env": {
    "production": {},
    "development": {
      "plugins": [
        [
          "dva-hmr", {
            "container": ".react-app",
            "quiet": false
          }
        ]
      ]
    }
  },
  "comments": false
}
